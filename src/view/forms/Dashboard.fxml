<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" style="-fx-background-color: yellow;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Dashboard">
   <children>
      <ImageView fitHeight="313.0" fitWidth="308.0" layoutX="46.0" layoutY="130.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/foodCity.png" />
         </image>
      </ImageView>
      <Button fx:id="btnCustomer" layoutX="14.0" layoutY="470.0" mnemonicParsing="false" onAction="#btnCustomer_OnAction" text="Customer" />
      <Button fx:id="btnItem" layoutX="158.0" layoutY="470.0" mnemonicParsing="false" onAction="#btnItem_OnAction" prefHeight="31.0" prefWidth="84.0" text="Item" />
      <Button fx:id="btnOrder" layoutX="302.0" layoutY="470.0" mnemonicParsing="false" onAction="#btnOrder_OnAction" prefHeight="31.0" prefWidth="84.0" text="Order" />
      <Label layoutX="19.0" layoutY="14.0" text="FOOD CITY" textFill="#171616">
         <font>
            <Font name="System Bold" size="70.0" />
         </font>
      </Label>
      <Button fx:id="btnLogOut" layoutX="148.0" layoutY="542.0" mnemonicParsing="false" onAction="#btnLogOut_OnAction" style="-fx-background-color: black;" text="Log Out" textFill="#fffefe">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
